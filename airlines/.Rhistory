getmaxlen <- function(seq, char){
lennum <- rep(0, length(seq))
s <- 1
for (i in 1:length(lennum)){
if (i != length(lennum)){
if (seq[i] != seq[i + 1]){
lennum[i] <- s
s <- 1
}
else s <- s + 1
}
else lennum[i] <- s
}
return(max(lennum[which(seq == char)]))
}
